{
  "name": "{{name}}",
  "version": "0.0.2",
  "keywords": [
    "vue",
    "vuejs",
    "vuex",
    "vue-router",
    "vue-i18n",
    "vue-resource",
    "fullstack",{{#if_eq mock 'backend'}}
    "mongodb",
    "mongoose",{{/if_eq}}
    "socket.io",
    "element",
    "element-ui"
  ],
  "description": "{{description}}",
  "author": "{{author}}",
  "private": false,
  "scripts": {
    "client": "node {{#if_eq mock 'backend'}}client/{{/if_eq}}build/dev-server.js",{{#if_eq mock 'backend'}}
    "server": "./node_modules/.bin/nodemon --watch server server/app.js",{{/if_eq}}{{#if_eq mock 'mock'}}
    "mock": "./node_modules/.bin/nodemon --watch mock mock/index.js",{{/if_eq}}
    "dev": "npm run client|npm run {{#if_eq mock 'backend'}}server{{/if_eq}}{{#if_eq mock 'mock'}}mock{{/if_eq}}",
    "build": "node {{#if_eq mock 'backend'}}client/{{/if_eq}}build/build.js",
    {{#unless i18nEn}}"remove:i18nEn": "node tasks/replaceI18n.js",
    {{/unless}}{{#if_eq mock 'mock'}}"remove:mock": "node tasks/mock.js",
    {{/if_eq}}"lint": "eslint --ext .js,.vue {{#if_eq mock 'backend'}}client/{{/if_eq}}src"
  },
  "dependencies": {
    {{#if_eq mock "backend"}}
    "body-parser": "^1.18.2",
    "composable-middleware": "^0.3.0",
    "compression": "^1.7.2",
    "cookie-parser": "^1.4.3",
    "ejs": "^2.5.2",
    {{/if_eq}}
    "element-ui": "^2.3.3",
    {{#if_eq mock "backend"}}
    "express": "^4.16.3",
    "express-jwt": "^5.3.1",
    {{/if_eq}}{{#if_eq mock "mock"}}
    "fms": "0.2.0",
    {{/if_eq}}{{#if_eq mock "backend"}}
    "jsonwebtoken": "^7.1.9",
    {{/if_eq}}
    "lodash": "^4.17.5",
    {{#if_eq mock "backend"}}
    "method-override": "^2.3.7",
    {{/if_eq}}{{#if_eq mock "mock"}}
    "mockjs": "^1.0.1-beta3",
    {{/if_eq}}
    "nodemon": "^1.17.3",
    {{#if_eq mock "backend"}}
    "mongoose": "^5.0.13",
    {{/if_eq}}
    "nprogress": "^0.2.0",
    {{#if_eq mock "backend"}}
    "passport": "^0.3.2",
    "passport-local": "^1.0.0",
    {{/if_eq}}
    "socket.io": "^2.1.0",
    "socket.io-client": "^2.1.0",
    {{#if_eq mock "backend"}}
    "socketio-jwt": "^4.5.0",
    {{/if_eq}}
    "vue": "^2.5.16",
    "vue-i18n": "^7.6.0",
    "vue-resource": "^1.5.0",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1",
    "vuex-router-sync": "^5.0.0"
  },
